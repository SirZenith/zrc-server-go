			<string><![CDATA[CREATE USER ARCAEA IDENTIFIED BY ARCAEA;

CREATE TABLE ARCAEA.PARTNER ( 
	PART_ID              number(2)  NOT NULL,
	STEP                 char(119)  NOT NULL,
	FRAG                 char(119)  NOT NULL,
	CONSTRAINT PK_PARTNER_PAT_ID PRIMARY KEY ( PART_ID ) 
 );

COMMENT ON COLUMN ARCAEA.PARTNER.STEP IS 'step value of different levels of a character.';

COMMENT ON COLUMN ARCAEA.PARTNER.FRAG IS 'frag values of different levels of an character.';

CREATE TABLE ARCAEA.SONG ( 
	SONG_ID              varchar2(30)  NOT NULL,
	JP_TRANS             varchar2(30)  ,
	EN_TRANS             varchar2(40)  NOT NULL,
	BASE_PTT             float(3)  NOT NULL,
	CONSTRAINT PK_SONG_SONG_ID PRIMARY KEY ( SONG_ID ) 
 );

CREATE TABLE ARCAEA.UNCAP_COND ( 
	PART_ID              number(2)  NOT NULL,
	COND_1               number(1)  NOT NULL,
	COND_2               number(1)  ,
	CONSTRAINT PK_UNCAP_COND_PART_ID PRIMARY KEY ( PART_ID ) ,
	CONSTRAINT FK_UNCAP_COND_PARTNER FOREIGN KEY ( PART_ID ) REFERENCES ARCAEA.PARTNER( PART_ID )  
 );

COMMENT ON TABLE ARCAEA.UNCAP_COND IS 'uncap conditions of partners.';

COMMENT ON COLUMN ARCAEA.UNCAP_COND.COND_1 IS 'required core type (type code) to uncap.';

COMMENT ON COLUMN ARCAEA.UNCAP_COND.COND_2 IS 'required core type (type code) to uncap.';

CREATE TABLE ARCAEA.WORLD_MAP ( 
	MPA_ID               varchar2(30)  NOT NULL,
	CHAPTER_ID           number(2)  NOT NULL,
	STAMINA_COST         number(1)  NOT NULL,
	STEP_COUNT           number(3)  NOT NULL,
	IS_REPEATABLE        char(1)  ,
	HAVE_LEGACY_BOOST    char(1)  ,
	MAP_DATA             raw(10240)  NOT NULL,
	CONSTRAINT PK_WORLD_MAP_MPA_ID PRIMARY KEY ( MPA_ID ) 
 );

CREATE TABLE ARCAEA.USER ( 
	UID                  number(9)  NOT NULL,
	EMAIL                varchar2(320)  NOT NULL,
	NAME                 varchar2(20)  NOT NULL,
	PWDHASH              varchar2(100)  NOT NULL,
	FRIEND_CODE          number(9)  NOT NULL,
	TICKET               integer DEFAULT 0 NOT NULL,
	CURR_MAP             varchar2(30)  ,
	PTT                  float(4,2) DEFAULT 0 NOT NULL,
	STAMINA              number(2)  NOT NULL,
	NEXT_FRAGE2STAM_TIME integer  NOT NULL,
	STAMINA_RECOVER_TIME integer  ,
	CORE_0               number(2)  ,
	CORE_1               number(2)  ,
	CORE_2               number(2)  ,
	CORE_3               number(2)  ,
	CORE_4               number(2)  ,
	CONSTRAINT PK_USER_UID PRIMARY KEY ( UID ) ,
	CONSTRAINT IDX_USER_UID UNIQUE ( UID ) ,
	CONSTRAINT IDX_USER_FRIEND_CODE UNIQUE ( FRIEND_CODE ) ,
	CONSTRAINT IDX_USER_EMAIL UNIQUE ( EMAIL ) ,
	CONSTRAINT FK_USER_WORLD_MAP FOREIGN KEY ( CURR_MAP ) REFERENCES ARCAEA.WORLD_MAP( MPA_ID )  
 );

COMMENT ON TABLE ARCAEA.USER IS 'sotre user infomation';

COMMENT ON COLUMN ARCAEA.USER.UID IS 'user id for system to recognize each user, distribute in registration date order.';

COMMENT ON COLUMN ARCAEA.USER.FRIEND_CODE IS 'fix length (9 digits) code, used when player add another player as friend.';

COMMENT ON COLUMN ARCAEA.USER.TICKET IS 'ticket for buying songs in game.';

COMMENT ON COLUMN ARCAEA.USER.CURR_MAP IS 'map that player is currently working on.';

COMMENT ON COLUMN ARCAEA.USER.STAMINA IS 'stamina for play in world mode.';

COMMENT ON COLUMN ARCAEA.USER.NEXT_FRAGE2STAM_TIME IS 'time in second left until next frage-to-stamina exchange is unlocked.';

COMMENT ON COLUMN ARCAEA.USER.STAMINA_RECOVER_TIME IS 'time in second left until stamina is fully recover.';

CREATE TABLE ARCAEA.PART_POSS_INFO ( 
	UID                  number(9)  NOT NULL,
	PART_ID              number(3)  NOT NULL,
	PART_LEVEL           number(2)  NOT NULL,
	CURR_EXP             number(3)  NOT NULL,
	IS_UNCAPED           char(1)  ,
	IS_SKILL_SEALED      char(1)  ,
	CONSTRAINT FK_PART_POSS_INFO_PARTNER FOREIGN KEY ( PART_ID ) REFERENCES ARCAEA.PARTNER( PART_ID )  ,
	CONSTRAINT FK_PART_POSS_INFO_USER FOREIGN KEY ( UID ) REFERENCES ARCAEA.USER( UID )  
 );

CREATE INDEX IDX_PART_POSS_IN_UID ON ARCAEA.PART_POSS_INFO ( UID );

CREATE TABLE ARCAEA.PLAY_RECORD ( 
	ID                   integer  NOT NULL,
	UPLOAD_DATE          date  ,
	UID                  number(9)  NOT NULL,
	SONG_ID              varchar2(30)  NOT NULL,
	SHINEY_PERF          number(4)  NOT NULL,
	PERF                 number(4)  NOT NULL,
	FAR                  number(4,0)  NOT NULL,
	LOST                 number(4,0)  NOT NULL,
	SCORE                number(8,0)  NOT NULL,
	PTT                  float(0,6)  NOT NULL,
	CONSTRAINT PK_PLAY_RECORD_ID PRIMARY KEY ( ID ) ,
	CONSTRAINT FK_PLAY_RECORD_SONG FOREIGN KEY ( SONG_ID ) REFERENCES ARCAEA.SONG( SONG_ID )  ,
	CONSTRAINT FK_PLAY_RECORD_USER FOREIGN KEY ( UID ) REFERENCES ARCAEA.USER( UID )  
 );

CREATE INDEX IDX_PLAY_RECORD_UID ON ARCAEA.PLAY_RECORD ( UID );

CREATE TABLE ARCAEA.RECNET_PLAYED ( 
	RECORD_ID            integer  NOT NULL,
	UID                  number(9)  NOT NULL,
	CONSTRAINT PK_PLAY_RECORD_ID_0 PRIMARY KEY ( RECORD_ID ) ,
	CONSTRAINT FK_RECNET_PLAYED_PLAY_RECORD FOREIGN KEY ( RECORD_ID ) REFERENCES ARCAEA.PLAY_RECORD( ID )  ,
	CONSTRAINT FK_RECNET_PLAYED_USER FOREIGN KEY ( UID ) REFERENCES ARCAEA.USER( UID )  
 );

CREATE INDEX IDX_PLAY_RECORD_UID_0 ON ARCAEA.RECNET_PLAYED ( UID );

CREATE TABLE ARCAEA.SONG_POSSESION ( 
	UID                  number(9)  NOT NULL,
	SONG_ID              varchar2(30)  NOT NULL,
	CONSTRAINT FK_SONG_POSSESION_SONG FOREIGN KEY ( SONG_ID ) REFERENCES ARCAEA.SONG( SONG_ID )  ,
	CONSTRAINT FK_SONG_POSSESION_USER FOREIGN KEY ( UID ) REFERENCES ARCAEA.USER( UID )  
 );

CREATE INDEX IDX_SONG_POSSESI_UID ON ARCAEA.SONG_POSSESION ( UID );

CREATE TABLE ARCAEA.BEST_PLAYED ( 
	RECORD_ID            integer  NOT NULL,
	UID                  number(9)  NOT NULL,
	CONSTRAINT PK_PLAY_RECORD_ID_1 PRIMARY KEY ( RECORD_ID ) ,
	CONSTRAINT FK_BEST_PLAYED_PLAY_RECORD FOREIGN KEY ( RECORD_ID ) REFERENCES ARCAEA.PLAY_RECORD( ID )  ,
	CONSTRAINT FK_BEST_PLAYED_USER FOREIGN KEY ( UID ) REFERENCES ARCAEA.USER( UID )  
 );

CREATE INDEX IDX_PLAY_RECORD_UID_1 ON ARCAEA.BEST_PLAYED ( UID );
]]></string>
